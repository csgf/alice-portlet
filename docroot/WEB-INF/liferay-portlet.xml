<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE liferay-portlet-app PUBLIC "-//Liferay//DTD Portlet Application 6.0.0//EN" "http://www.liferay.com/dtd/liferay-portlet-app_6_0_0.dtd">
<liferay-portlet-app>
  <portlet>
    <portlet-name>alice_portlet</portlet-name>
    
    
    
    <friendly-url-mapper-class>com.liferay.portal.kernel.portlet.DefaultFriendlyURLMapper</friendly-url-mapper-class>
    <friendly-url-mapping>alice_portlet</friendly-url-mapping>
    <friendly-url-routes>it/infn/ct/alice-friendly-url-routes.xml</friendly-url-routes>
    <instanceable>false</instanceable>
    <header-portlet-css>/css/main.css</header-portlet-css> 
    <header-portlet-css>/css/layout.css</header-portlet-css>
    <header-portlet-css>/css/jquery.dataTables.css</header-portlet-css>
    <header-portlet-css>/css/tabAnalisi.css</header-portlet-css>
    <header-portlet-css>/css/tableStyle.css</header-portlet-css>
    <header-portlet-javascript>/js/jquery-1.11.1.min.js</header-portlet-javascript>
    <header-portlet-javascript>/js/jquery.dataTables.js</header-portlet-javascript>
   <css-class-wrapper>alice-portlet</css-class-wrapper>
    
  </portlet>
  <role-mapper>
    <role-name>administrator</role-name>
    <role-link>Administrator</role-link>
  </role-mapper>
  <role-mapper>
    <role-name>guest</role-name>
    <role-link>Guest</role-link>
  </role-mapper>
  <role-mapper>
    <role-name>power-user</role-name>
    <role-link>Power User</role-link>
  </role-mapper>
  <role-mapper>
    <role-name>user</role-name>
    <role-link>User</role-link>
  </role-mapper>
</liferay-portlet-app>
